<?xml version="1.0" encoding="ISO-8859-1"?>

<bitfields product="phpkd_vblvb">
	<bitfielddefs>
		<group name="phpkd_vblvb">
			<group name="protocols">
				<bitfield name="http"  >1</bitfield>
				<bitfield name="https" >2</bitfield>
				<bitfield name="ftp"   >4</bitfield>
			</group>
			<group name="bbcodes">
				<bitfield name="basic" >1</bitfield>
				<bitfield name="color" >2</bitfield>
				<bitfield name="size"  >4</bitfield>
				<bitfield name="font"  >8</bitfield>
				<bitfield name="align" >16</bitfield>
				<bitfield name="list"  >32</bitfield>
				<bitfield name="link"  >64</bitfield>
				<bitfield name="code"  >128</bitfield>
				<bitfield name="php"   >256</bitfield>
				<bitfield name="html"  >512</bitfield>
				<bitfield name="quote" >1024</bitfield>
				<bitfield name="hide"  >2048</bitfield>
				<bitfield name="charge">4096</bitfield>
			</group>
			<group name="thread_modes">
				<bitfield name="opened"    >1</bitfield>
				<bitfield name="closed"    >2</bitfield>
				<bitfield name="moderated" >4</bitfield>
				<bitfield name="sticky"    >8</bitfield>
				<bitfield name="deleted"   >16</bitfield>
				<bitfield name="redirected">32</bitfield>
			</group>
			<group name="post_modes">
				<bitfield name="opened"   >1</bitfield>
				<bitfield name="moderated">2</bitfield>
				<bitfield name="deleted"  >4</bitfield>
			</group>
			<group name="thread_punishs">
				<bitfield name="close"   >1</bitfield>
				<bitfield name="unstick" >2</bitfield>
				<bitfield name="moderate">4</bitfield>
				<bitfield name="delete"  >8</bitfield>
				<bitfield name="move"    >16</bitfield>
			</group>
			<group name="post_punishs">
				<bitfield name="moderate">1</bitfield>
				<bitfield name="delete"  >2</bitfield>
			</group>
			<group name="staff_reports">
				<bitfield name="pm"    >1</bitfield>
				<bitfield name="email" >2</bitfield>
				<bitfield name="thread">4</bitfield>
				<bitfield name="reply" >8</bitfield>
			</group>
			<group name="user_reports">
				<bitfield name="pm"    >1</bitfield>
				<bitfield name="email" >2</bitfield>
			</group>
		</group>
	</bitfielddefs>
</bitfields>

